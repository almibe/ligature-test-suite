(assert-equal
  (assertions [a b c])
  (assertions 
    { @ a
      b c }))

(assert-equal
  (assertions [a b c] [a e f])
  (assertions 
    { @ a
      b c
      e f }))
  
(assert-equal
  (assertions [a : A])
  (assertions 
    { @ a
      : A }))

(assert-equal
  (assertions [a : A] [a : B])
  (assertions 
    { @ a
      : [A B] }))

(assert-equal
  (assertions [a b c] [c d e])
  (assertions 
    { @ a
      b { @ c 
          d e } }))
