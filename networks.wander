assert-equal
  (network [a b c])
  (network 
    { @id a
      b c }),

assert-equal
  (network [a b c] [a e f])
  (network 
    { @id a
      b c
      e f }),
    
assert-equal
  (network [a : A])
  (network 
    { @id a
      : A }),

assert-equal
  (network [a : A] [a : B])
  (network 
    { @id a
      : [A B] }),

assert-equal
  (network [a b c] [c d e])
  (network 
    { @id a
      b { @id c 
          d e } }),
