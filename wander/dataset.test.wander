Assert.equal "Simple Dataset literal" 
    { `a` `b` `c` }
    { `a` `b` `c` },

Assert.equal "Simple Dataset literal with two Statements" 
    { `d` `e` `f`, `a` `b` `c` }
    { `a` `b` `c`, `d` `e` `f` },

Assert.equal "Dataset literal with Value list" 
    { `a` `b` `c`, `a` `b` `f` }
    { `a` `b` [`c`, `f`] },

Assert.equal "Dataset literal with Entity definition" 
    { `a` `b` `c`, `a` `d` `e` }
    { `a` { `b` `c`, `d` `e` } },
